<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="dbaasConsumer-10-V2" class="com.francetelecom.clara.cloud.activation.plugin.dbaas.infrastructure.DBaasConsumer20Impl">
		<property name="dbaasStub" ref="dbaasProxy-10-V2"></property>
		<property name="mvnDao" ref="mvnDao"></property>
		<property name="dbaasGroupName" value="${test.dbaas10.groupname}" />
		<property name="timeout" value="${test.dbaas10.timeout}" />
	</bean>
	
	<!-- Factory de stub pour le WebService -->
	<bean id="dbaasFactory-10-V2" class="org.apache.cxf.jaxws.JaxWsProxyFactoryBean">
		<property name="serviceClass" value="com.orange.clara.cloud.dbaas.wsdl.service.DbaasApiRemote" />
		<property name="address" value="${test.dbaas10.url}" />
		<property name="username" value="${test.dbaas10.user}" />
		<property name="password" value="${test.dbaas10.password}" />
	</bean>

	<!-- instanciation des proxy de services -->
	<bean id="dbaasProxy-10-V2" class="com.orange.clara.cloud.dbaas.wsdl.service.DbaasApiRemote"
		factory-bean="dbaasFactory-10-V2" factory-method="create" />

</beans>